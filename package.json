  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "cpr": "^3.0.1",
    "rimraf": "^2.6.1",
    "rollup": "^0.56.3",
    "rollup-plugin-babel": "^3.0.3"
  },
  "peerDependencies": {
    "@types/react": "^16.0.38",
    "react": "^16.2.0"
  }
}
